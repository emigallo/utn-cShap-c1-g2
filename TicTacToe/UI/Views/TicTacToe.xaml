<Window x:Class="UI.TicTacToe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Grid.Column="0" Grid.Row="0" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>
        <Button Grid.Column="1" Grid.Row="0" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>
        <Button Grid.Column="2" Grid.Row="0" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>
        <Button Grid.Column="0" Grid.Row="1" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>
        <Button Grid.Column="1" Grid.Row="1" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>
        <Button Grid.Column="2" Grid.Row="1" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>
        <Button Grid.Column="0" Grid.Row="2" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>
        <Button Grid.Column="1" Grid.Row="2" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>
        <Button Grid.Column="2" Grid.Row="2" Content="" Style="{StaticResource Cell}" Click="ButtonPress"/>


    </Grid>
</Window>
